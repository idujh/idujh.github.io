*{margin:0;padding:0;box-sizing:border-box;}
body{font-family:'Noto Sans KR',sans-serif;
font-size:14px;
font-weight:500;
color:#333;}
li{list-style:none;}
a,a:hover{text-decoration:none;
font-weight:500;
color:#333;}
img{border:0;border:none;
vertical-align:middle;}
.quickmenu{position:absolute;left:50%;margin-left:640px;margin-top:150px;width:100px;cursor:pointer;text-align:center;}
.quickmenu>img{width:90%;float:left;margin-left:5%;}
.quickmenu ul{float:left;width:90%;text-align:center;margin-top:10px;
background-color:#FFF;border-top:10px solid #c10a28;margin-left:5%;
box-shadow:0px 0px 10px rgba(0,0,0,0.3);}
.quickmenu ul>li{float:left;width:100%;}
.quickmenu ul>li>a{width:90%;margin-left:5%;
display:block;line-height:200%;border-top:1px solid #acacac;}
.quickmenu ul>li>a>img{width:60px;}
.quickmenu ul>li>a>img:last-child{width:100%;}
.quickmenu .quick_first{border:none;}
.quickmenu .quick_last{width:100%;margin:0;border:none;padding:0;}

.wrap{width:100%;}
.headerwrap{width:100%;}
.headertop{width:100%;max-width:1180px;margin:0 auto;background-color:#FFF;position:relative;height:60px;}
.headertop h1{position:absolute;left:50%;z-index:1;margin-left:-38.5px;}
.movetext{float:left;width:50%;color:#c10a28;line-height:60px;
background-color:rgba(255,255,255,0.1);
text-align:center;font-family:'jeju Hallasan',sans-serif;font-size:20px;font-weight:bold;}
.search100{position:absolute;left:0;line-height:60px;}
.login{position:absolute;right:0;line-height:60px;}
.login img{margin-bottom:10px;}
.headerbottom{width:100%;background-color:#c10a28;height:60px;}
.headerbottominner{width:1180px;margin:0 auto;position:relative;}
.allmenu{position:absolute;left:0;line-height:60px;padding:0 10px 0 10px;text-align:center;}
.allmenu_m{display:none;}
.content_mwrap{display:none;}
.allmenu_pbg{position:absolute;top:120px;width:100%;left:0;background-color:#313131;z-index:2;display:none;height:500px;}
.allmenu_p{position:absolute;top:60px;left:0;width:100%;z-index:3;display:none;}
.allmenu_p ul{width:17.5%;float:left;line-height:200%;margin-top:20px;}
.allmenu_p ul>li{width:100%;float:left;}
.allmenu_p ul>li:last-child{margin-bottom:20px;}
.allmenu_p ul>li>a{color:#fff;font-size:16px;font-weight:400;display:block;}
.allmenu_p ul>li>ul{width:100%;float:left;margin:0;}
.allmenu_p ul>li>ul>li>a{color:#aaaaaa;font-size:16px;font-weight:400;}
.allmenu_p ul>li>ul>li>a:hover{color:#ffc227;}
.allmenu_p_img{width:30%;float:left;margin-top:20px;}
.allmenu_p_img img{width:100%;}
.gnb{width:40%;float:left;margin-left:5%;margin-right:5%;line-height:57px;}
.gnb>li{float:left;width:25%;text-align:center;position:relative;}
.gnb>li>a{display:block;color:#fff;font-weight:500;font-size:16px;}
.gnb>li>a:hover{border-bottom:3px solid orange;}
.gnb>li>ul{width:800px;position:absolute;top:50px;left:0;z-index:1;display:none;}
.gnb>li>ul>li{float:left;margin-left:40px;}
.gnb>li>ul>li>a{display:block;font-size:16px;font-weight:400;}
.gnbbg{position:absolute;top:120px;left:0;width:100%;height:40px;
background-color:#fafafa;z-index:1;display:none;}
.maskwrap{width:100%;height:480px;background:url(image/pattern.png) 0 0 repeat;}
.mask{max-width:1180px;width:100%;height:460px;margin:0 auto;overflow:hidden;
position: relative;}
.pagerwrap{position: absolute;bottom:0px;left:50%;display:block;width:200px;
margin-left:-100px;}
.pager{width:100%;text-align: center;line-height: 22px;}
.pager>a{display:inline-block;width:13px;height:13px;background-color:#666666;
border-radius:7.5px;margin:0 2px;}
.pager>a.on00{background-color: #ff5151;width:26px;}
.next{position: absolute;right:0;width:26px;opacity:0.6;bottom:-1px;}
.next>img{width:100%;}
.prev{position: absolute;left:0;width:26px;opacity:0.6;bottom:0px;}
.prev>img{width:100%;}
.slider{width:5000%;position: relative;margin-top:20px;}
.slider>li{float:left;}
.slider>li>a{display:block;}
.slider>li>a>img{width:100%;}
.order{float:left;width:40%;margin-left:10%;}
.order a{display:inline-block;width:32.66%;height:40px;background-color:#FFF;color:#c10a28;
margin-right:1%;border-radius:10px;text-align:center;line-height:40px;margin-top:10px;
font-weight:500;font-size:15px;}
.order .mr0{margin-right:0;}
.order a img{height:18px;margin-right:5px;vertical-align:middle;margin-bottom:2px;}
.order a .h20{height:20px;}
.order .order1{color:#000;background-color:#febe47;}
.content{width:100%;}
.content>ul{width:100%;max-width:1180px;margin:0 auto;}
.content>ul>li{width:49%;float:left;margin-right:2%;margin-top:2%;}
.content>ul .mr00{margin-right:0;}
.content>ul>li>a{display:block;box-shadow:1px 3px 5px #666;}
.content1{width:100%;background-color:#e9ebec;float:left;margin-top:20px;}
.content1>ul{width:100%;max-width:1180px;margin:0 auto;}
.content1>ul>li{width:49%;float:left;margin-right:2%;margin-top:2%;position:relative;}
.content1>ul .mr000{margin-right:0;}
.content1>ul>li>a{display:block;background-color:#FFF;}
.content1>ul>li>a>img{width:100%;}
.noticewrap{width:100%;float:left;}
.notice{width:1180px;margin:0 auto;overflow:hidden;}
.noticeinner{width:49%;background-color:#fff;margin-right:2%;height:120px;
margin-top:2%;position:relative;float:left;margin-bottom:2%;}
#noticeinner{margin-right:0;}
.noticeinner h2{float:left;width:100%;padding:10px;text-align:center;}
.text00{width:90%;display:block;margin-left:5%;background-color:#f0f0f0;color:#6a6a6a;
float:left;font-size:16px;font-weight:400;padding:10px;box-shadow:1px 3px 5px #666;}
.plus{position:absolute;right:0;top:0;display:block;background-color:#c1c1c1;width:30px;
height:30px;color:#fff;font-size:28px;text-align:center;line-height:30px;}
.footerwrap{width:100%;float:left;padding-bottom:20px;}
.footer{max-width:1180px;width:100%;margin:0 auto;}
.footericon{width:70%;margin-left:15%;margin-top:20px;}
.footericon>a{display:block;float:left;width:14.2857%;text-align:center;line-height:300%;
font-weight:400;color:#6a6a6a;}
.footericon>a>img{width:46px;}
.call{float:left;overflow:hidden;width:100%;margin-top:20px;}
.call>a{display:block;float:left;width:49.95%;padding:10px;background-color:#acacac;
margin-right:0.1%;color:#fff;font-weight:normal;}
.call>a>img{margin-right:10px;}
.call .call_m0{margin:0;}
.call>a>span{font-weight:500;float:right;margin-right:20px;color:#c8102e;}
.terms{width:100%;float:left;border-top:1px solid #ddd;margin-top:20px;
border-bottom:1px solid #ddd;padding:10px 0;}
.terms>a{display:block;font-weight:normal;color:#6a6a6a;float:left;width:12.5%;text-align:center;}
.terms>a:first-child{text-align:left;}
.terms>a:last-child{text-align:right;color:#c8102e;}
.f_bottom{width:100%;float:left;margin-top:20px;position:relative;margin-bottom:20px;}
.f_bottom h6{float:left;}
.f_bottom p{float:left;margin-left:10px;font-weight:normal;font-size:13px;}
.f_bottom p span{font-weight:600;}
.f_app{position:absolute;right:0;top:0;width:220px;}
.f_app>a{float:left;}
.f_app>a>img{width:100%;}
.f_app>a:last-child{margin-top:10px;}



