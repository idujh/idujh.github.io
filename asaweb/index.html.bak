<script>
$(document).ready(function(){

//헤더
$(window).on('load',function(){
var botheight=$('.header_bot').height()
$('.gnb>li>a').css({height:botheight,lineHeight:botheight+'px'})
$('.gnb>li>ul').css({top:botheight})
$('.gnbbg').css({top:botheight})
$('.mask').css({height:$('.slide>li>a>img').height()})
$('.prev,.next').css({marginTop:-$('.prev,.next').height()/2})
})

$('.gnb>li>a').on('mouseenter focus',function(){
$('.gnb>li>ul').stop().fadeOut(400)
$(this).next().stop().fadeIn(800)
$('.gnbbg').stop().fadeIn(800)
$('.gnb>li>a').removeClass('on')
$(this).addClass('on')
})

$('.header').on('mouseleave blur',function(){
$('.gnb>li>ul,.gnbbg').stop().fadeOut(400)
})

//컨텐츠
var now=1
var total=$('.slide>li').length
var loop=setInterval(function(){
if (now==total)
{
	now=0
}
now++
$('.slide>li').stop().fadeOut(1000)
$('.slide>li').eq(now-1).stop().fadeIn(1200)
$('.pager>a').removeClass('on')
$('.pager>a').eq(now-1).addClass('on')
},8000)

$('.next').on('click',function(){
if (now==total)
{
	now=0
}
now++
$('.slide>li').stop().fadeOut(1000)
$('.slide>li').eq(now-1).stop().fadeIn(1200)
$('.pager>a').removeClass('on')
$('.pager>a').eq(now-1).addClass('on')
return false
})

$('.prev').on('click',function(){
if (now==0)
{
	now=total
}
now--
$('.slide>li').stop().fadeOut(1000)
$('.slide>li').eq(now-1).stop().fadeIn(1200)
$('.pager>a').removeClass('on')
$('.pager>a').eq(now-1).addClass('on')
return false
})

$('.lat').hide()
$('.lat:first').show()
$('.main_board_title>a').each(function(index){
$(this).on('click',function(){
$('.main_board_title>a').removeClass('on')
$(this).addClass('on')
$('.lat').fadeOut(400)
$('.lat').eq(index).fadeIn(800)
})
})

var min=1
var max=$('.main_board_title>a').length
var board=setInterval(function(){
if (min==max)
{
	min=0
}
min++
$('.main_board_title>a').removeClass('on')
$('.main_board_title>a').eq(min-1).addClass('on')
$('.lat').fadeOut(400)
$('.lat').eq(min-1).fadeIn(800)
},6000)

$('.main_board_title,.lat').on('mouseenter focus',function(){
clearInterval(board)
})

$('.main_board_title,.lat').on('mouseleave blur',function(){
clearInterval(board)
board=setInterval(function(){
if (min==max)
{
	min=0
}
min++
$('.main_board_title>a').removeClass('on')
$('.main_board_title>a').eq(min-1).addClass('on')
$('.lat').fadeOut(400)
$('.lat').eq(min-1).fadeIn(800)
},6000)
})

$('.main_product_pager>a').each(function(index){
$(this).on('click',function(){
$('.main_product ul').stop().animate({left:-index*270})
$('.main_product_pager>a').removeClass('on')
$(this).addClass('on')
return false
})
})

var pro1=0
var pro2=$('.main_product ul>li').length
var pro3=setInterval(function(){
if (pro1==pro2-1)
{
	pro1=-1
}
pro1++
$('.main_product ul').stop().animate({left:-pro1*270})
$('.main_product_pager>a').removeClass('on')
$('.main_product_pager>a').eq(pro1).addClass('on')
},6000)




})//ready
</script>
