<script>
$(document).ready(function(){

//헤더
$(window).on('load',function(){
var gheight=$('.gnb').height()
$('.gnb>li>ul').css({top:gheight+1})
})
var header=true
var loop=setInterval(function(){
if (header)
{
$('body,html').css({backgroundColor:'#6994a5'})
$('.header').addClass('on')
header=false
}else{
$('body,html').css({backgroundColor:'#699da5'})
$('.header').removeClass('on')
header=true
}
},10000)

$('.gnb>li>a').on('mouseenter focus',function(){
$('.gnb>li>a').removeClass('on')
$(this).addClass('on')
$('.gnb>li>ul').fadeOut()
$(this).next().fadeIn()
})

//메인컨텐츠
var now=1
var total=$('.slide>li').length
var loop1=setInterval(function(){
$('.slide').stop().animate({left:'-=100%'},function(){
$('.slide>li').first().appendTo('.slide')
$('.slide').css({left:0})
})
if (now==total)
{
	now=0
}
now++
$('.bar').css({width:'100%'})
$('.bar'+now).css({width:0})
$('.num>p').removeClass('on')
$('.num'+now).addClass('on')
},6000)

$('.next').on('click',function(){
$('.slide').stop().animate({left:'-=100%'},function(){
$('.slide>li').first().appendTo('.slide')
$('.slide').css({left:0})
})
if (now==total)
{
	now=0
}
now++
$('.bar').css({width:'100%'})
$('.bar'+now).css({width:0})
$('.num>p').removeClass('on')
$('.num'+now).addClass('on')
return false
})

$('.slide>li').click(function(){
var href=$(this).attr('data-href')
location.href=href
})

//푸터
$("#select").selectmenu();






})//ready
</script>
