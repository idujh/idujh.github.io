/* ν—¤λ” */
*{margin:0;padding:0;box-sizing:border-box;}
html{font-size:62.5%;}
body{font-family:'Noto Sans KR',sans-serif;font-size:1.4em;color:#333;
background-color: #699da5;}
li{list-style:none;}
a{text-decoration:none;color:#333;}
img{vertical-align: top;border:none;max-width:100%;}
a:hover{}
a:focus{}
.wrap{}
.wrap_inner{width:1024px;margin:0 auto;}
.header{padding:20px 50px 0 10px;position: relative;
background:url(image/mainbg1.jpg) 0 0 no-repeat;padding-bottom:435px;
-webkit-background-size: cover;
background-size: cover;}
.header.on{background:url(image/mainbg2.jpg) 0 0 no-repeat;
-webkit-background-size: cover;
background-size: cover;}
.header::after{content:'';clear:both;display:block;}
.header h1{float:left;}
.sitemap{position: absolute;right:50px;top:10px;}
.sitemap>a{font-size:1.0rem;color:#fff;text-align: right;padding-left:30px;}
.sitemap>a:hover{text-decoration: underline;}
.gnbwrap{width:670px;float:right;margin-top:20px;}
.gnb{position: relative;}
.gnb::after{content:'';clear:both;display:block;}
.gnb>li{float:left;width:20%;}
.gnb>li>a{display:block;color:#fff;font-size:1.6rem;font-weight:600;
text-align: center;padding:6px 22px;}
.gnb>li>a.on{background-color:rgba(0,0,0,0.3);}
.gnb>li>ul{position: absolute;left:0;top:49px;background-color:rgba(0,0,0,0.3);
width:670px;overflow: hidden;}
.g1{display:none;}
.g2{display:none;}
.g3{display:none;}
.g4{display:none;}
.g5{display:none;}
.gnb>li>ul>li{float:left;}
.gnb>li>ul>li>a{color:#fff;font-size:1.2rem;display:block;padding:10px 20px;
position: relative;}
.gnb>li>ul>li>a.on{background-color:rgba(0,0,0,0.3);}
.gnb>li>ul>li:not(:last-child) a::after{position: absolute;right:-2px;top:14px;width:2px;
height:12px;background-color: #ddd;content:'';}

/* ν‘Έν„° */
.footer{padding:40px 0 80px 10px;background-color: #668083;}
.footer::after{content:'';clear:both;display:block;}
.footer h6{float:left;}
.information{margin-left:20px;float:left;}
.information>a{color:#ddd;font-size:1.2rem;text-align:center;float:left;display:block;}
.copyright{margin-top:30px;}
.copyright>p{font-size:1.1rem;color:#ddd;line-height: 200%;}
.form{float:left;margin-left:10px;}
#select{}
#my .ui-selectmenu-button.ui-button {
    text-align: left;
    white-space: nowrap;
    width: 140px;
}
#my .ui-button {
    padding:4px;
    display: inline-block;
    position: relative;
    line-height: normal;
    margin-right:0;
    cursor: pointer;
    vertical-align: middle;
    text-align: center;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    overflow: visible;
}
#my .ui-widget {
    font-family: 'Noto Sans KR',sans-serif;
    font-size: 1.2rem;
}
#my .ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default, .ui-button, html .ui-button.ui-state-disabled:hover, html .ui-button.ui-state-disabled:active {
    font-weight: 500;
    color: #2779aa;
}