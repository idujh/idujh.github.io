@charset "utf-8";
/* SIR 지운아빠 */

/* 새글 스킨 (latest) */
.lt_pc {float:left;margin-left:20px}
.lt {
    position: absolute;
    float: left;
    margin-bottom: 0;
    padding-bottom: 0;
    width: 354px;
    height: 150px;
    border-bottom: 1px solid #e9e9e9;
    bottom: 60px;
    left: 16px;
    background-color: #fff;
    padding: 0 10px;
}
.lt ul {
    margin: 0;
    padding: 0;
    list-style: none;
}
.lt li {padding:3px 0}
.lt .lt_title {
    display: block;
    padding: 0;
    font-size: 1.4em;
    border-bottom: 3px solid saddlebrown;
    text-align: left;
}
.lt .lt_more {position:absolute;top:10px;right:0}
.lt .cnt_cmt {display:inline-block;margin:0 0 0 3px;font-weight:bold}