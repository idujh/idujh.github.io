<script type="text/javascript">

$(document).ready(function(){

$('.guide>li>a').each(function(index){
	var icon=$(this).find('img').attr('src')
	$(this).on({
	mouseenter:function(){
   $(this).css({color:'#116d75'})
   $(this).find('img').attr({src:icon.replace('.png','_on.png')})
	},
    mouseleave:function(){
	  $(this).css({color:'#333'})
   $(this).find('img').attr({src:icon.replace('_on.png','.png')})
	},
	})//on
	})//each

$('.gnb>li>a').each(function(index){
$(this).on({
mouseenter:function(){
$('.gnbwrap').slideDown(500)
$('.bg').show()
$('.next1,.prev1').hide()
$('.next,.prev').hide()
$('.gnbtitle').css({background:'none'})
$(this).parent().find('.gnbtitle').css({background:'url(image/bg_gnb'+(index+1)+'.jpg) no-repeat'})
$('.gnbtitle').find('h2,p').hide()
$(this).parent().find('.gnbtitle').find('h2,p').show()
},
mouseleave:function(){
$('.gnbwrap').css({width:0})
},

})//on
})//each

$('.header').on({
mouseleave:function(){
	$('.gnbwrap').css({width:1200})
$('.gnbwrap').slideUp(500)
$('.bg').hide()
$('.next1,.prev1').show()
$('.next,.prev').show()

},
})//on

var now=1
var all=$('.visual>ul>li').length

$('<div class="page"></div>').prependTo('.visual')
$('.page').html('<b>'+now+'</b>/'+all)


$('.next').on('click',function(){
	$('.visual').stop().animate({left:'-=1200'},50,function(){
	$('.visual>ul>li').first().appendTo('.visual>ul')
    $('.visual').css({left:0})
	})//ani
if (now==all)
{
	now=0
}
now++
$('.page').html('<b>'+now+'</b>/'+all)
return false
})//click

$('.prev').on('click',function(){
$('.visual>ul>li').last().prependTo('.visual>ul')
$('.visual').css({left:-1200})
$('.visual').stop().animate({left:'+=1200'},50)
if (now==1)
{
	now=all+1
}
now--
$('.page').html('<b>'+now+'</b>/'+all)
return false
})//click

$('.quick>ul>li>a').on({
mouseenter:function(){
$(this).css({backgroundColor:'#ffd200'})
},
mouseleave:function(){
$(this).css({backgroundColor:'#fff'})
},
})//on

$('.sub>li>a').on({
mouseenter:function(){
$(this).css({color:'#ffd200',fontWeight:'bold'})
},
mouseleave:function(){
$(this).css({color:'black',fontWeight:'normal'})
},
})//on

$('.next1').on('click',function(){
$('.event>ul').stop().animate({left:'-=400'},50,function(){
$('.event>ul>li').first().appendTo('.event>ul')
$('.event>ul').css({left:0})
})//ani
return false
})//click

$('.prev1').on('click',function(){
$('.event>ul>li').last().prependTo('.event>ul')
$('.event>ul').css({left:-400})
$('.event>ul').stop().animate({left:'+=400'},50)
return false
})

var loop=setInterval(function(){$('.visual').stop().animate({left:'-=1200'},1000,function(){
	$('.visual>ul>li').first().appendTo('.visual>ul')
    $('.visual').css({left:0})
	})//ani
	if (now==all)
{
	now=0
}
now++
$('.page').html('<b>'+now+'</b>/'+all)
},7000)//interval

var move=setInterval(function(){$('.event>ul').stop().animate({left:'-=400'},100,function(){
$('.event>ul>li').first().appendTo('.event>ul')
$('.event>ul').css({left:0})
})//ani
},8000)//interval
setInterval(function(){
$('.carinner').stop().animate({left:'-=1200'},function(){
$('.car').first().appendTo('.carinner')
$('.carinner').css({left:0})
})
},6000)

$('.service ul>li>a').each(function(index){
$(this).on('mouseenter focus',function(){
$('.service ul>li>a>img').css({opacity:1})
$(this).find('img').css({opacity:0.1})
$('.service ul>li>a').css({background:'none'})
$(this).css({background:'#3db5d8'})
$('.servicetext1').stop().animate({top:0,opacity:1},400)
$(this).find('.servicetext1').stop().animate({top:-70,opacity:0},400)
$('.servicetext2').stop().animate({top:0,opacity:0},400)
$(this).find('.servicetext2').stop().animate({top:-70,opacity:1},400)
})
})
$('#select>option').on('click',function(){
var val=$(this).val()
location.href=val
})
$('.language').on('click',function(){
$('body,html').stop().animate({scrollTop:$(document).height()},800,function(){
$('#select').css({border:'3px solid red'})
$('#select').stop().animate({opacity:0.5},2000,function(){
$('#select').stop().animate({opacity:1},2000,function(){
$('#select').css({border:'none'})
})
})
})
})


})//ready

</script>

