*{margin:0;padding:0;box-sizing:border-box;}
html{font-size:62.5%;}
body{font-family:'Noto Sans KR',sans-serif;color:#333;font-size:1.4em;
letter-spacing:-1px;}
li{list-style:none;}
a{text-decoration:none;color:#333;}
img{vertical-align: middle;max-width:100%;border:none;}
a:hover{color:#d00b17;}
a:focus{color:#d00b17;}
.wrap{min-width:1500px;}
.header{position: absolute;z-index:1;min-width:1500px;width:100%;}
.headerinner{padding:12px 0;text-align: center;}
.headerinner::after{content:'';clear:both;display:block;}
#headerinner2{display:none;background-color:rgba(255,255,255,0.9);
width:100%;top:0;position: fixed;min-width:1500px;}
.gnb{margin-left:40px;float:left;}
.gnb>li{float:left;position: relative;}
.gnb>li>a{display:block;font-weight:500;font-size:1.5rem;
width:52px;}
.gnb>li>a>img{padding-bottom:4px;}
.gnb>li>ul{width:80px;box-shadow:0 0 5px #666;position: absolute;top:22px;
left:-10px;background-color: #fff;padding:10px;line-height: 300%;display:none;}
.gnb>li>ul>li>a{display:block;}
.gnb>li>ul>li>a:hover{border-bottom:1px solid #d00b17;}
.headerinner h1{position: absolute;top:0;left:50%;margin-left:-42.5px;}
.login1{float:right;margin-right:40px;}
.login1>a{margin-left:10px;font-size:1.2rem;font-weight:300;}
.search_cart{clear:both;float:right;margin-right:40px;margin-top:10px;
position: relative;}
.search_cart>a{padding:0 4px;}
.searchwrap{position: fixed;top:50px;right:80px;border: 2px solid #d00b17;
padding:30px;background-color: #fff;width:600px;text-align: left;z-index:10;
display:none;}
#login1{position: relative;}
.search_form{position: relative;}
.search_form label{text-indent:-999px;font-size:0;display:none;}
.search_form input[type=search]{border:none;width:100%;color:#333;
border-bottom: 1px solid #000;padding:10px 40px 10px 10px;font-size:1.8rem;}
.search_form input[type=search]::placeholder{font-weight:400;color:#000;
font-family:'Noto Sans KR',sans-serif;font-size:1.8rem;}
.search_form a{position: absolute;right:10px;top:10px;}
.search_text{padding:15px 25px;}
.search_text h2{font-size:1.4rem;font-weight:500;letter-spacing:0;
color:#666;}
.search_text ul{padding:0;margin:0;margin-top:10px;line-height: 240%;}
.search_text ul>li>a{display:block;font-size:1.6rem;letter-spacing:0;
font-weight:300;}
.allmenu{position: fixed;top:10px;left:10px;background-color: #fff;
width:1000px;z-index:1;box-shadow:0 0 5px #333;display:none;}
.allmenu::after{display:block;clear:both;content:'';}
.allmenu>ul{width:83%;float:left;}
.allmenu>ul>li{float:left;width:14.2857%;position: relative;}
.allmenu>ul>li>a::after{content:'';position: absolute;height:680px;
right:0;top:70px;width:1px;background-color: #e0e0e0;}
.allmenu>ul>li>a{display:block;font-size:1.7rem;padding:10px 0;
font-weight:500;letter-spacing:0;}
.allmenu>ul>li>ul{line-height: 180%;margin:20px 0 40px 0;}
.allmenu>ul>li>ul>li>a>h2{font-size:1.5rem;font-weight:500;margin-bottom:6px;}
.allmenu>ul>li>ul>li>a{display:block;font-weight:300;font-size:1.3rem;
letter-spacing:-0.5px;position: relative;}
.allmenu_stroke{position: absolute;bottom:-20px;left:50%;margin-left:-6%;
width:12%;height:1px;background-color: #e0e0e0;}
.allmenu_close{position: absolute;right:20px;top:10px;padding:0 5px;}
.allmenu_bigstroke{position: absolute;left:82%;top:0;
width:2%;height:100%;background-color: #f0f0f0;}
.allmenu_gnbstroke{position: absolute;top:50px;left:0;margin-left:1%;
width:80%;height:1px;background-color: #aaa;}
.allmenu_lastlist{float:left;width:17%;line-height: 300%;margin-top:200px;}
.allmenu_lastlist>ul>li>a{display:inline-block;font-size:1.4rem;padding:10px 0;
font-weight:500;letter-spacing:0;}
.allmenu_lastlist>li>a>h2{font-size:1.7rem;font-weight:500;margin-bottom:6px;}
.allmenu_lastlist>li>a>h2>span{color:#e0e0e0;font-weight:400;}

.content{}
.mask{height:790px;position: relative;}
.slide>li>a{position: absolute;display:block;left:50%;margin-left:-960px;}
.slide>li>a>img{height:790px;width:100%;}
.pager{position: absolute;bottom:40px;left:60px;font-weight:600;font-size:1.6rem;
letter-spacing:1px;z-index:1;}
.pager>span{color:#d00b17;font-size:2.0rem;font-weight:bold;}
.gallery1{padding:70px 0;}
.gallery1 ul{width:1500px;margin:0 auto;padding:0 20px;overflow: hidden;}
.gallery1 ul>li{float:left;}
.gallery1 ul>li>a{display:block;width:473.333px;margin-right:20px;overflow: hidden;}
.gallery1 ul>li>a>img{width:100%;}
.content .mr0{margin:0;}
.gallery2{padding:50px 0;background-color: #f0f0f0;}
.gallery2 h2{text-align: center;margin-bottom:30px;}
.gallery2 ul{width:1500px;margin:0 auto;padding:0 20px;overflow: hidden;
position: relative;}
.gallery2 ul>li{float:left;}
.gallery2 ul>li>a{display:block;width:284px;margin-right:10px;position: relative;}
.pinkbd{position: absolute;left:20px;top:0;width:284px;
border: 3px solid #d00b17;background-color:rgba(0,0,0,0.4);text-align: center;
display:table-cell;vertical-align: middle;}
.pinkbd>a{display:inline-block;margin:45% auto;height:10%;color:#e0e0e0;
padding:10px;}
.readmore{margin-top:20px;text-align: center;}
.readmore a{display:inline-block;border-bottom:1px solid #000;font-weight:200;
font-size:1.2rem;color:#000;letter-spacing:0px;}
.readmore a:hover{border-bottom-color:#d00b17;color:#d00b17;}
.quick{text-align: center;padding:70px 0;}
.quick h3{display:inline-block;}
.quick ul{width:1200px;margin:0 auto;margin-top:40px;height:300px;}
.quick ul::after{clear:both;display:block;content:'';}
.quick ul>li{float:left;}
.quick ul>li>a{display:block;width:200px;height:200px;margin-left:80px;float:left;
position: relative;}
.quick ul>li>a>span{display:block;margin-top:220px;letter-spacing:0;
font-size:1.5rem;font-weight:300;line-height: 220%;}
.quick ul>li>a>span>span{font-weight:400;font-size:1.7rem;}
.quick .quick1{background:url(../image/quick_menu_1.png) 0 0 no-repeat;
-webkit-background-size:cover;
background-size:cover;}
.quick .quick2{background:url(../image/quick_menu_2.png) 0 0 no-repeat;
-webkit-background-size:cover;
background-size:cover;}
.quick .quick3{background:url(../image/quick_menu_3.png) 0 0 no-repeat;
-webkit-background-size:cover;
background-size:cover;}
.quick .quick4{background:url(../image/quick_menu_4.png) 0 0 no-repeat;
-webkit-background-size:cover;
background-size:cover;}

/*ν‘Έν„°*/
.footer{border-top:1px solid #aaaaaa;}
.footerinner{width:1200px;margin:0 auto;padding:40px; 0;}
.footertop{overflow: hidden;}
.footertop>h6{float:left;width:200px;}
.footertoplist{float:left;width:200px;}
.footertoplist>h6{font-size:1.4rem;font-weight:500;}
.footertoplist ul{margin-top:10px;line-height: 160%;}
.footertoplist ul>li>a{font-size:1.2rem;font-weight:300;display:block;}
.footertoplist .fw{font-weight:500;}
#footertoplist{float:left;width:320px;border-left:1px solid #e0e0e0;
padding:0 0 30px 40px;}
#footertoplist p{font-size:1.2rem;font-weight:300;}
#footertoplist .tellnumber{padding:4px 4px;font-size:2.2rem;letter-spacing:0;
font-weight:400;}
.footerbottom{overflow: hidden;padding-top:20px;border-top:1px solid #e0e0e0;margin-top:20px;}
.footerbottom_sns{width:200px;float:left;}
.footerbottom_sns a{margin-right:10px;}
.footerbottom_copyright{width:520px;float:left;font-size:1.2rem;
line-height: 200%;}
.footerbottom_mark{float:left;width:400px;}
.footerbottom_mark a{float:right;display:block;margin-left:10px;}
.movebtn{position: absolute;
 bottom:20px;right:20px;width:75px;height:76px;}
.movebtn .topbtn{width:75px;height:38.5px;display:block;outline:none;
background:url(../image/quickbtn.png) 0 0 no-repeat;}
.movebtn .topbtn:hover{background-position: 0 -80px;}
.movebtn .downbtn{width:75px;height:38.5px;display:block;outline:none;
background:url(../image/quickbtn.png) 0 -38.5px no-repeat;}
.movebtn .downbtn:hover{background-position: 0 -118.5px;}




















