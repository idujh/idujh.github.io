<script>
$(document).ready(function(){

//메인비주얼인터발
$('.slide>li>a').hide()
$('.slide>li>a').first().show()

var min=1
var max=$('.slide>li').length
var loop=setInterval(function(){
if (min==max)
{
	min=0
}
min++
$('.slide>li>a').fadeOut(1000)
$('.slider'+min).fadeIn(1200)
$('.pager>span').html(min)
},6000)

//헤더 스크롤이벤트
$(window).on('scroll',function(){
var top=$(window).scrollTop()
setTimeout(function(){
$('#headerinner2').css({top:top,position:'absolute'})
},4000)
if (top>100)
{
$('#headerinner1').fadeOut()
$('#headerinner2').fadeIn()
}else{
$('#headerinner2').fadeOut()
$('#headerinner1').fadeIn()
}
})

//메인 카드섹션 마우스오버
$('.gallery1 ul>li>a').on({
mouseenter:function(){
$(this).find('img').stop().animate({scale:1.03})
},
mouseleave:function(){
$(this).find('img').stop().animate({scale:1})
}
})



})//ready
</script>

