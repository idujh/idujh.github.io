<script>
$(document).ready(function(){
//스타일 js
$('.section1_banner').css({marginTop:$('.tab_mask').height()+60})

//폼이벤트
$('.search_form').on('mouseenter focus',function(){
$('.search_form #search').css({borderColor:'#ff4191'})
$('.search_form .button').css({backgroundPosition:'-33px -40px'})   
})
$('.search_form').on('mouseleave blur',function(){
$('.search_form #search').css({borderColor:'#b3b3b3'})
$('.search_form .button').css({backgroundPosition:'0 -40px'})
$('.search_form #search').blur()
})

//헤더아이콘 이벤트
var icon=setInterval(function(){
var random=Math.floor(Math.random()*$('.icon>a').length)
$('.icon>a').eq(random).stop().animate({scale:1.2,rotateX:'+=360deg',perspective:400},400,'easeOutBounce',function(){
$(this).stop().animate({scale:1},400,'easeOutBounce') 
}) 
},10000)
 
//메인 슬라이드 이벤트
$('.slide>li').hide()
$('.slide>li:first()').show()
var now=1
var total=$('.slide>li').length
var loop=setInterval(function(){
if (now==total)
{
	now=0
}
now++
$('.slide>li').stop().fadeOut(1000)
$('.slider'+now).stop().fadeIn(1200)
$('.pagerinner>a').removeClass('on')
$('.pagerinner>a').eq(now-1).addClass('on')
},4000)

$('.slide_text_left a').on('mouseenter focus',function(){
$(this).siblings().stop().animate({opacity:0.5})
$(this).stop().animate({opacity:1})
})
$('.slide_text_left').on('mouseleave blur',function(){
$('.slide_text_left a').stop().animate({opacity:1})
})

$('.pagerinner>a').each(function(index){
$(this).on('click',function(){
$('.slide>li').stop().fadeOut(1000)
$('.slide>li').eq(index).stop().fadeIn(1200)
$('.pagerinner>a').removeClass('on')
$(this).addClass('on')
return false
})
})

var stop=true;
$('.stop_start').on('click',function(){
if (stop)
{
$(this).css({backgroundPosition:'-62px -230px'})
clearInterval(loop)
stop=false;
}else{
$(this).css({backgroundPosition:'-82px -230px'})
loop=setInterval(function(){
if (now==total)
{
	now=0
}
now++
$('.slide>li').stop().fadeOut(1000)
$('.slider'+now).stop().fadeIn(1200)
$('.pagerinner>a').removeClass('on')
$('.pagerinner>a').eq(now-1).addClass('on')
},4000)
stop=true;
}
return false
})

//카드섹션 첫번째
$('.latest_view>li>a').append('<div class="listening"></div>')
$('.listening').html('대표곡&nbsp;듣기')
$('.latest_view>li>a').on('mouseenter focus',function(){
$(this).find('.listening').show('highlight',{color:'#ff4191'},600)
})
$('.latest_view>li>a').on('mouseleave blur',function(){
$(this).find('.listening').fadeOut()
})

$('.latesttitle_list>a').each(function(index){
$(this).on('click',function(){
$('.tab_mask').stop().fadeOut(1000)
$('.tab_mask').eq(index).stop().fadeIn(1200)
$('.latesttitle_list>a').removeClass('on')
$(this).addClass('on')
return false
})
})

$('.tab_mask').each(function(index){
$(this).find('.tab_pager>a').each(function(index){
$(this).on('click',function(){
$(this).parent().prev().stop().animate({left:index*-1172})
$(this).siblings().removeClass('on')
$(this).addClass('on')
return false;
})
})
$(this).on('mouseenter focus',function(){
$(this).find('.tab_prev').show()
$(this).find('.tab_next').show()
})
$(this).on('mouseleave blur',function(){
$(this).find('.tab_prev').hide()
$(this).find('.tab_next').hide()
})
var min=0
var max=3
$(this).find('.tab_next').on('click',function(){
if (min==max)
{
	min=-1
}
min++
$(this).prev().prev().find('a').removeClass('on')
$(this).prev().prev().find('a').eq(min).addClass('on')
$(this).prev().prev().prev().stop().animate({left:min*-1172})
return false;
})

$(this).find('.tab_prev').on('click',function(){
if (min==0)
{
	min=max+1
}
min--
$(this).prev().find('a').removeClass('on')
$(this).prev().find('a').eq(min).addClass('on')
$(this).prev().prev().stop().animate({left:min*-1172})
return false;
})
})

//실시간챠트
$('.chart_pager>a').each(function(index){
$(this).on('click',function(){
$('.chart_move').stop().animate({left:index*-1172},400,'easeOutBack')
$('.chart_pager>a').removeClass('on')
$(this).addClass('on')
return false
})
})










})//ready
</script>
