<script>
$(document).ready(function(){
//헤더
$(window).on('load',function(){
$('.gnb>li>a').each(function(index){
$(this).on('mouseenter focus',function(){
var move=$(this).position()
var color=['#d03231','#ccd031','#6cd130','#31c0d0','#2f3fd2','#cd30d1']
$('.gnb>li>a').css({color:'#333',borderBottomColor:'transparent'})
$(this).css({color:color[index],borderBottomColor:color[index]})
$('.gnbbg,.gnb>li>ul').stop().slideDown()
})
})
})
$('.header').on('mouseleave blur',function(){
$('.gnbbg,.gnb>li>ul,.gnb>li>ul>li>ul').stop().slideUp()
})
$('.gnb>li>ul>li>a').on('mouseenter focus',function(){
$('.gnb>li>ul>li>ul').stop().slideUp(200)
$(this).next().stop().slideDown(200)
})

var Language=true;
$('.Language_btn').click(function(){
if (Language)
{
$('.Language_show').slideDown()
$(this).css({backgroundImage:'url(image/korean.png),url(image/flag_arrow1.png)'})
Language=false
}else{
$('.Language_show').slideUp()
$(this).css({backgroundImage:'url(image/korean.png),url(image/flag_arrow.png)'})
Language=true
}
})
$('.Language_btn').blur(function(){
$('.Language_show').slideUp()
$(this).css({backgroundImage:'url(image/korean.png),url(image/flag_arrow.png)'})
Language=true
})
$('.search_btn').on('click',function(){
$('.form').toggle('fold')
})
$('.search_wrap').on('mouseleave blur',function(){
$('.form').hide('fold')
})

//컨텐츠
$('.slide>li').hide()
$('.slide>li').eq(2).show()
$('.slide_select>a').each(function(index){
$(this).click(function(){
$('.slide_select>a').css({opacity:1})
$(this).css({opacity:0.8})
$('.slide>li').hide()
$('.slide>li').eq(index).show()
$('.slide_text').css({opacity:0,left:0})
$('.slide_text').eq(index).delay(600).animate({left:'2%',opacity:1},600,'linear')
$('.slide>li>img').css({scale:1.1,opacity:0})
$('.slide>li>img').stop().animate({scale:1,opacity:1},1000,'linear')
return false
})
})

var sun=setInterval(function(){
$('.oci_blank').stop().animate({marginBottom:20},1000,function(){
$('.oci_blank').stop().animate({marginBottom:0},1000)
})
},2000)

$(window).on('load resize',function(){
$('.slide2').bxSlider({auto:true})
var ulheight=$('.gnb>li>ul').height()
var width=$(window).width()
var sheight=$('.slide_select').height()/6
var ssheight=$('.slide2 .image img').height()
var ssswidth=$('.content2_top_right a').width()/2
var sssheight=$('.content2_top_right a').height()/2
$('.slide>li').stop().animate({width:width})
$('.slide_select').stop().animate({top:-sheight})
$('.slide2 .text').height(ssheight)
$('.content2_top_right').height(ssheight)
$('.content2_top_right a').css({marginLeft:-ssswidth,marginTop:-sssheight})
$('.gnbbg').css({top:$('.header').height(),height:ulheight})
$('.gnb>li>ul').css({top:$('.header_bottom').height()})
})

//푸터
var familysite=true;
$('.familysite>a').click(function(){
if (familysite)
{
$(this).css({backgroundImage:'url(image/flag_arrow.png)'})
$('.familysite_list').stop().slideDown()
familysite=false;
}else{
$(this).css({backgroundImage:'url(image/flag_arrow1.png)'})
$('.familysite_list').stop().slideUp()
familysite=true;
}
return false
})
$('.familysite>a').blur(function(){
$(this).css({backgroundImage:'url(image/flag_arrow1.png)'})
$('.familysite_list').stop().slideUp()
familysite=true;
})

})//ready
</script>
