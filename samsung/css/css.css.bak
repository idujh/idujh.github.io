*{margin:0; padding:0;box-sizing:border-box;}
li { list-style-type:none; }
img{border:0;border: none; max-width:100%;vertical-align:top;}
body{font-family:'Noto Sans KR',sans-serif;font-size:16px;
color: #222222;}
a{text-decoration: none;color:#222222;}
a:hover{color:#1f53a7;}
a:focus{color:#1f53a7;}
.wrap{width:100%;}
.headerwrap{width:100%;padding-bottom:40px;height:107px;}
.header{width:1200px;margin:0 auto;border-bottom: 1px solid #e5e5e5;padding-top:20px;height:67px;}
.header>h1{float:left;}
.gnb{float:left;margin-left:80px;}
.gnb>li{float:left;position: relative;}
.gnb>li>a{display:block;padding-right:48px;font-weight:500;color:#4f4f4f;}
.gnb>li>a.on99{color:#1f53a7;}
.gnbstroke{position: absolute;width:0;height:2px;background-color: #1f53a7;
left:0;bottom:-5px;}
.gnb>li>ul{position: absolute;width:800px;top:53px;}
.gnb>li>ul>li{float:left;}
.gnb>li>ul>li>a{padding-right:30px;display:block;font-weight:300;}
.g1{display:none;}
.g2{display:none;}
.g3{display:none;}
.g4{display:none;}
.g5{display:none;}
.language{float:right;}
.language>a{display:inline-block;margin-left:20px;font-size:0.6em;color:#4f5964;}
.language>a.on00{color:#1f53a7;font-weight:600;}
.content{width:100%;}
.mask{width:100%;min-width:1900px;
margin:0 auto;height:600px;overflow: hidden;}
.maskinner{width:1200px;height:600px;position: relative;margin:0 auto;}
.slide{position: relative;width:6000px;}
.slide>li{position: absolute;}
.slidebtn{position: absolute;bottom:0;right:0;z-index:1;overflow: hidden;}
.slidebtn>a{float:left;display:block;background:url(../image/btn_visual_cnt.png);
width:46px;height:46px;background-repeat:no-repeat;}
.slidebtn>a:hover{border: 1px solid #ecffff;}
.slidebtn>a:focus{border: 1px solid #ecffff;}
.slidebtn>a.active{border: 1px solid #ecffff;}
.slidebtn .prev{background-position: 0 0;float:left;}
.slidebtn .stop{background-position: -46px 0;float:left;}
.slidebtn .next{background-position: -92px 0;float:right;}
.slidebtn .play{background-position: -138px 0;float:left;}
.pager{position: absolute;bottom:40px;left:0;}
.pager>a{display:block;float:left;width:16px;height:16px;border: 2px solid #fff;
border-radius:100%;margin-right:8px;cursor:default;}
.pager>a.on88{background-color: #fff;}
.mediawrap{width:100%;min-width:1900px;
background-color: #1c97fe;padding:60px 0;}
.media{width:1200px;margin:0 auto;overflow: hidden;}
.mediainner{width:400px;float:left;}
.mediainner h2{font-size:1.8em;color:#fff;font-weight:normal;position: relative;
width:100%;overflow: hidden;z-index:1;}
.mediainner a{display:block;width:100%;margin-top:20px;overflow: hidden;}
.mediainner a .image{float:left;width:120px;margin-top:2px;}
.mediainner a .text{float:left;width:240px;color:#fff;font-size:0.8em;line-height: 130%;font-weight:300;letter-spacing:-0.5px;}
.mediainner a .text span{font-size:1.1em;font-weight:500;}
.mediainner a .text .view{width:100%;float:left;margin-top:20px;}
.mediainner a .text .view img{vertical-align: middle;margin-left:12px;}
.hfirst{width:45%;float:left;text-align:left;cursor:pointer;}
.hlast{width:55%;float:left;text-align: left;cursor:pointer;padding-left:10px;}
.mediainner h2 .stroke{position: absolute;left:0;top:0;width:100%;height:100%;
z-index:-1;}
#news1{display:block;}
#news2{display:none;float:left;}
#stroke1{display:block;}
#stroke2{display:none;}
#mediaright>h2>span.on77{color:#ddd;}
.productwrap{width:100%;min-width:1900px;
background:url(../image/brandSite_bg.png) 0 0 no-repeat;
text-align: center;padding:60px 0;
-webkit-background-size: cover;
background-size:cover ;}
.productwrap>h2{font-size:2em;text-align: center;}
.productwrap ul{width:1200px;margin:0 auto;margin-top:40px;overflow: hidden;}
.productwrap ul>li{float:left;width:25%;}
.productwrap ul>li>a{display:block;font-size:1.2em;font-weight:600;}
.productwrap ul>li>a>.products1{margin-top:20px;float:left;width:100%;}
.productwrap ul>li>a>.products1>span{font-size:0.7em;font-weight:400;}
.productwrap ul>li>a>.products2{margin-top:40px;float:left;width:100%;}
.brandwrap{width:100%;padding:60px 0 20px 0;min-width:1900px;}
.brand{width:1200px;margin:0 auto;}
.brand>h2{font-size:2em;text-align: center;}
.brand ul{width:100%;text-align: center;overflow: hidden;
padding:20px 0;margin-top:20px;}
.brand ul>li{width:380px;margin:0 10px;float:left;}
.brand ul>li>a{display:block;text-align: left;box-shadow:0px 5px 12px #ddd;
overflow: hidden;}
.brandtextwrap{padding:30px 24px 30px 24px;float:left;}
.brandtext1{font-size:1.2em;font-weight:600;width:100%;float:left;opacity:0.9;}
.brandtext2{font-size:0.8em;font-weight:400;width:100%;float:left;opacity:0.8;}
.brandtext3{font-size:0.8em;font-weight:300;width:100%;float:left;opacity:0.6;
margin-top:20px;}
.brandtext4{margin-top:20px;float:left;}
.footerwrap{width:100%;min-width:1900px;border-top:1px solid #ddd;}
.footer{width:1200px;margin:0 auto;padding:20px 0;overflow: hidden;}
.footer>h6{float:left;width:200px;text-align: center;padding-top:20px;}
.footer>h6>img{width:117px;}
.personalinformation{float:left;width:915px;overflow: hidden;}
.personalinformation>a{float:left;display:block;font-size:0.75em;color:#a3a3a3;
font-weight:300;}
.personalinformation>p{clear:both;font-size:0.75em;color:#a3a3a3;margin-top:10px;
float:left;}
.webaccessibility{float:left;}