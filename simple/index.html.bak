<script>
$(document).ready(function(){
$('.tab>a').append('<div class="over"></div>')
$('<h2 class="title"></h2>').prependTo('.over')
$('.tab').each(function(index){
$(this).find('a').each(function(index){
var alt=$(this).find('img').attr('alt')
$(this).find('.over').find('.title').html(alt+'<span><br>전체퍼블리싱+스크립트+하드코딩<span>')
$(this).on('mouseenter focus',function(){
$('.over').stop().animate({opacity:0})
$(this).find('.over').stop().animate({opacity:1})
})
})
})

$('.btn>a').each(function(index){
$(this).on('click',function(){
$('.btn>a').removeClass('on')
$(this).addClass('on')
$('.tab').hide()
$('.tab').eq(index).show(400)
return false
})
})






$(window).on('load resize',function(){
var width=$(window).width()
var height=$(window).height()
var th=$('.tab>a').height()
$('.over').height(th)
$('.log').html('width:'+width+'height:'+height)
})
})//readey
</script>
