})
})
}
})

var now=1
var total=$('.btn>a').length
$('.next').on('click',function(){
$(this).effect('highlight',{color:'#5f9ea0'})
if (now==total)
{
	now=0
}
now++
$('.btn>a').eq(now-1).click()
return false
})
$('.prev').on('click',function(){
$(this).effect('highlight',{color:'#5f9ea0'})
if (now==0)
{
	now=total
}
now--
$('.btn>a').eq(now-1).click()
return false
})








