<script>
$(document).ready(function(){
$('body').jpreLoader({
showSplash:true,
splashID:'#loader',
loaderVPos:'50%',
splashVPos:'50%',
autoclose:true,
showPercentage:true,
closeBtnText:'Start!'
})

$('.wrap>h1').typer({
strings: [
'웹퍼블리셔/ui개발/엄재환','Web Publisher','Cross Browsing','Makeup Development','Responsive Web','Web Standard','Html5/Css3/Js','Ui Developer'
],
				typeSpeed: 200, 
                backspaceSpeed: 20, 
                backspaceDelay: 10000, 
                repeatDelay: 1000, 
                repeat: true, 
                autoStart: true,
                startDelay: 1000, 
});

$('.bgm2').liveBorder({
top:true,
bottom:true,
left:true,
right:true
});

$('body,html').tooltip();
$('.tab>a').attr({title:'기여도:퍼블리싱100%, 스크립트100%'})
$('#tab1>a').eq(3).attr({title:'기여도:퍼블리싱100%, 스크립트100%, 디자인50%'})
$('#tab2>a').eq(3).attr({title:'기여도:퍼블리싱100%, 스크립트100%, 디자인50%'})
$('#tab3>a').eq(1).attr({title:'기여도:퍼블리싱100%, 스크립트100%, 디자인50%'})
$('.tab>a').append('<div class="over"></div>')
$('<h2 class="title"></h2>').prependTo('.over')
$('.over').stop().animate({opacity:0,scale:0,rotateX:'180deg'},1000)
$('.tab').each(function(index){
$(this).find('a').each(function(index){
var alt=$(this).find('img').attr('alt')
$(this).find('.over').find('.title').html(alt+'<span><br>전체퍼블리싱+스크립트+하드코딩<br>새창으로이동↗<span>')
$(this).on('mouseenter focus',function(){
/* var speak=$(this).attr('data-speak')
responsiveVoice.speak(speak,"Korean Female");
var audio1=new Audio('music/music1.mp3')
var audio2=new Audio('music/music2.mp3')
var audio3=new Audio('music/music3.mp3')
var audio=[audio1,audio2,audio3]
var random=Math.floor(Math.random()*audio.length)
audio[random].play()
audio[random].volume=0.3; */
$('.over').stop().animate({opacity:0,scale:0,rotateX:'180deg'},1000)
$('.title').css({opacity:0,left:50})
$(this).find('.over').stop().animate({opacity:1,scale:1,rotateX:'0deg'},400,function(){
$(this).find('.title').stop().animate({left:0,opacity:1})
})
})
$(this).on('click',function(){
responsiveVoice.speak('새창으로 이동',"Korean Female");
})
})
})

$('.btn>a').each(function(index){
$(this).on('click',function(){
var databtn=$(this).attr('data-btn')
responsiveVoice.speak(databtn,"Korean Female");
$('.btn>a').removeClass('on')
$(this).addClass('on')
$('.tab').hide(400)
$('.tab').eq(index).show(800)
$('.move_text').html(databtn)
$('.move_text').stop().fadeIn(2000,function(){
$(this).stop().fadeOut(2000)
})
return false
})
$(this).on('mouseenter focus',function(){
var bw=$(this).outerWidth()
var bh=$(this).outerHeight()
var pos=$(this).position()
$('.btnbd').stop().animate({width:bw,height:bh,left:pos.left,top:pos.top,opacity:1})
})
})

$('.btn').on('mouseleave blur',function(){
$('.btnbd').stop().animate({opacity:0})
})

var bgm1=new Audio('music/bgm1.mp3')
var bgm2=new Audio('music/bgm2.mp3')
var bgm3=new Audio('music/bgm3.mp3')
var bgm_All=[bgm1,bgm2,bgm3]
var random=Math.floor(Math.random()*bgm_All.length)
var bgm=true;
$('.bgm').on('click',function(){
if (bgm)
{
//bgm_All[random].currentTime=0;
bgm_All[random].play();
bgm_All[random].volume=0.5;
bgm_All[random].loop=true;
$('.bgm1').hide()
$('.bgm2').show()
bgm=false;
}else{
//bgm_All[random].currentTime=0;
bgm_All[random].pause();
bgm_All[random].volume=0.5;
bgm_All[random].loop=false;
$('.bgm2').hide()
$('.bgm1').show()
bgm=true;
}
return false
})

var hpbg1=['#c60606','#bec804','#c77505','#c89204']
var hpbg2=['#008040','#017e7b','#021e7d','#73037c']
var hpbd=setInterval(function(){
var r1=Math.floor(Math.random()*hpbg1.length)
var r2=Math.floor(Math.random()*hpbg2.length)
$('.hpbd').css({left:'-100%',backgroundColor:hpbg1[r1],width:'80%'})
$('.hpbd').stop().animate({left:'100%',backgroundColor:hpbg2[r2],width:'100%'},6000,'linear')
},7000)

$(window).on('load',function(){
responsiveVoice.speak('Welcome to the uhmjaehwan portfolio',"Japanese Female",{volume:0.4});
var width=$(window).width()
if (width>=1024)
{
$('.introtext').show()
$('.introtext>li').each(function(index){
