@import url(https://cdn.jsdelivr.net/font-notosans-kr/1.0.0-v1004/NotoSansKR-2350.css);
*{margin:0;padding:0;box-sizing:border-box;}
html{font-size:62.5%;}
body{font-family:'NotoSansKR',sans-serif;font-size:1.4em;color:#fff;
background:#050505 url(image/elegant-black-background-10.jpg) 50% 0 no-repeat;overflow-y: scroll;overflow-x: hidden;
background-attachment:fixed;}
li{list-style:none;}
a{text-decoration:none;color:#fff;}
img{vertical-align: top;border:none;max-width:100%;}
a:hover{}
a:focus{}

#my .ui-widget.ui-widget-content {
	border: 2px solid #333;
}
#my .ui-widget-content {
	border: 1px solid #dddddd;
	background: #f2f5f7;
	color: #333;
}
#my .ui-widget-content a {
	color: #333;
}

.wrap{width:70%;margin:0 auto;min-width:1300px;padding:50px 0;
max-width:1300px;position: relative;}
.wrap>h1{text-align: left;color:#ddd;font-weight:600;}
.wrap>h1>img{vertical-align: bottom;padding-right:4px;}
.btn{overflow: hidden;display:inline-block;padding:4px 0;position: relative;}
.btnbd{position: absolute;left:0;top:0;background-color: cadetblue;
border-radius:5px;width:0;height:0;opacity:0;}
.btn>a{display:block;float:left;padding:4px 10px;outline:none;position: relative;
z-index:1;}
.btn>a.on{background-color: cadetblue;border-radius:5px;color:#333;}
.box{margin-top:20px;}
.tab{overflow: hidden;display:none;position: relative;}
.tab>a{width:19%;display:block;float:left;overflow: hidden;margin-right:1%;
outline:none;position: relative;margin-bottom:1%;}
#tab1{display:block;}
.over{position: absolute;left:0;top:0;width:100%;height:100%;
padding-top:33.33%;opacity:0;border: 3px solid orangered;
background:rgba(0,0,0,0.6) url(image/chracter.png) no-repeat 100% 0;
-webkit-background-size: 40% 40%;
background-size: 40% 40%;}
.title{text-align: center;font-size:1.6rem;position: relative;opacity:0;left:50px;}
.title>span{font-size:1.2rem;}
.hp{position: fixed;right:0px;top:0px;padding:4px;background:#2e2e2e;
color:#fff;}
.bgm{padding:0 4px;color:#c60606;outline:none;}
.bgm>img{vertical-align: middle;padding-right:4px;}
.bgm2{color:#008040;display:none;}
.hpbd{position: absolute;left:-100%;top:0;width:80%;height:3px;
background-color: #c60606;font-size:0;text-indent:-999px;opacity:0.5;}
.introtext{position: absolute;top:0px;left:0;right:0;text-align: center;}
.introtext>li{position: absolute;top:-100px;left:0;right:0;text-align: center;
font-size:4.0rem;padding-top:14px;color:#ddd;text-shadow: 0 0 5px #333;}
.prev{position: fixed;left:0px;top:50%;-webkit-transform: translate(0, -50%);
-moz-transform: translate(0, -50%);outline:none;padding:13px;
-ms-transform: translate(0, -50%);
-o-transform: translate(0, -50%);
transform: translate(0, -50%);
}
.next{position: fixed;right:0px;top:50%;-webkit-transform: translate(0, -50%);
-moz-transform: translate(0, -50%);outline:none;padding:13px;
-ms-transform: translate(0, -50%);
-o-transform: translate(0, -50%);
transform: translate(0, -50%);
}
.np_btn:hover{background-color:#1f1f1f;}
.move_text{position: absolute;left:50%;top:50%;padding:5px;display:none;
color:#ddd;font-size:3rem;font-weight:400;border-radius:5px;-ms-transform: translate(-50%, -50%);transform: translate(-50%, -50%);background-color: #333;}



