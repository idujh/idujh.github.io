<!doctype html>
<html lang="ko">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width">
<!-- IE 브라우져 최신버젼 렌더링 모드 오류를 최대로 줄여줄수 있음. -->
<meta http-equiv="X-UA-Compatible" content="IE=edge">

<title>Document</title>

<style>
@import url(https://cdn.jsdelivr.net/font-notosans-kr/1.0.0-v1004/NotoSansKR-2350.css);
*{margin:0;padding:0;box-sizing:border-box;}
html{font-size:62.5%;}
body{font-family:'NotoSansKR',sans-serif;font-size:1.4em;color:#333;
background-color: #000;}
li{list-style:none;}
a{text-decoration:none;color:#333;}
img{vertical-align: top;border:none;max-width:100%;}
a:hover{}
a:focus{}

.movie_popup{background-color: #000;position: fixed;left:0;top:0;
right:0;bottom:0;z-index:99;display:none;}
.closebtn{position: absolute;right:10px;top:10px;}
.iframe_box{position: absolute;left:0;top:0;right:0;bottom:0;margin: auto;
width:100%;height:90%;}
.iframe_box iframe{position: absolute;left:0;top:0;width:100%;height:100%;
border:none;}
.wrap{max-width:1920px;margin:0 auto;}
.header{position: absolute;left:0;top:0;right:0;z-index:10;
padding:30px 20px 0 20px;}
.header_inner{max-width:1300px;margin:0 auto;padding:20px 40px;
position: relative;}
.header_inner.on{background-color: #fff;}
.header_inner::after{clear:both;content:'';display:block;}
.header_inner h1{float:left;}
.header_inner h1>a{display:block;background:url(image/logo.png) 0 0 no-repeat;
width:150px;height:32px;font-size:0;text-indent:-999px;}
.header_inner h1>a.on{background-position: -220px 0;
position: relative;z-index:1;}
.header_nav{float:right;}
.gnbwrap{float:left;}
.gnb>li{float:left;position: relative;}
.gnb>li>a{display:block;font-size:1.6rem;color:#fff;padding:0 30px;position: relative;z-index:1;}
.gnb>li>a.on1{color:#333;}
.gnb>li>a.on2{color:#e2211c;}
.gnb>li:not(:last-child)>a::after{content:'|';position: absolute;right:-2px;
color:#c2c2c2;}
.gnb>li>ul{width:100%;text-align: center;display:none;padding-top:20px;
position: relative;z-index:1;}
.gnb>li>ul>li>a{display:block;padding:4px 0;color:#666;}
.gnb>li>ul>li>a.on{color:#e2211c;}
.language{float:left;font-size:0;text-indent:-999px;display:block;width:47px;
height:32px;background:url(image/language_btn.png) 0 0 no-repeat;
position: relative;z-index:1;}
.language.on{background-position: -47px 0;}
.allmenu{float:left;font-size:0;text-indent:-999px;display:block;width:31px;height:32px;background:url(image/all_view.png) 0 0 no-repeat;margin-left:3px;
position: relative;z-index:1;}
.allmenu.on{background-position: -31px 0;}
.header_leftbg{position: absolute;left:-50px;top:0;display:none;z-index:0;}
.header_rightbg{position: absolute;right:-50px;bottom:0;display:none;
z-index:0;}
.mask{position: fixed;left:0;right:0;top:0;bottom:0;}
.slidebg{position: absolute;left:0;top:0;right:0;bottom:0;background-color: #000;
z-index:2;opacity:0;}
.slide{}
.slide>li{position: absolute;-webkit-background-size: cover;
background-size: cover;}
#slide1{background:url(image/visual07.jpg) 80% 50% no-repeat;left:0;}
#slide2{background:url(image/visual08.jpg) 70% 50% no-repeat;left:100%;}
#slide3{background:url(image/visual09.jpg) 80% 50% no-repeat;left:100%;}
.pager{position: absolute;bottom:100px;text-align: center;left:0;right:0;z-index:1;}
.pager>a{display:inline-block;width:50px;height:2px;margin-right:2px;
background-color: #fff;}
.pager>a.on{background-color: #e2211c;}
.content{position: absolute;top:100%;left:0;right:0;z-index:2;}
.topbtn{position: fixed;right:40px;bottom:40px;z-index:1;display:none;}
.content_inner{max-width:1300px;margin:0 auto;background-color: #fff;
position: relative;}
.scroll{background-color: #fff;position: relative;top:-37px;text-align: center;}
.scroll>img{position: relative;top:-17px;}
.news{padding:0 10%;overflow: hidden;}
.news>h2{width:15%;float:left;font-weight:600;}
.news>h2>span{display:block;}
.news>h2>a{display:block;width:23px;height:23px;margin-top:23px;font-size:0;
background:url(image/more_btn.png) 0 -23px no-repeat;text-indent:-999px;}
.news>h2>a:hover{background-position: 0 0;}
.news_list{float:left;width:85%;}
.news_list>li{float:left;width:33.33%;}
.news_list>li>a{display:block;padding:2% 10% 10%;}
.news_list>li:not(:last-child)>a{border-right:1px solid #ddd;}
.text_wrap{}
.text_wrap>h3{font-size:1.8rem;font-weight:400;}
.text_wrap>p{color:#888;padding-top:15%;}
.text_wrap>span{color:#888;padding-top:10%;display:block;font-size:1.2rem;}
.business{margin-top:5%;}
.business_list{overflow: hidden;}
.business_list>li{float:left;width:25%;}
.business_list>li>a{display:block;position: relative;}
.business_list>li>a>img{width:100%;}
.business_highlight{position: absolute;left:0;top:0;right:0;bottom:0;
background-color: #000;opacity:0;}
.business_highlight.on{opacity:0.5;}
.business_text{}
.business_text_inner{position: absolute;left:50%;top:50%;width:100%;text-align: center;-webkit-transform: translate(-50%, -50%);
-moz-transform: translate(-50%, -50%);
-ms-transform: translate(-50%, -50%);
-o-transform: translate(-50%, -50%);
transform: translate(-50%, -50%);}
.business_text_inner>h3{font-size:2.4rem;font-weight:400;color:#fff;}
.business_stroke{width:30px;height:2px;background-color: #fff;margin:5% auto;}
.business_text_inner>p{font-size:1.6rem;color:#fff;}
.movie_section{padding:10%;overflow: hidden;text-align: center;}
.movie_section>a:first-child{display:block;float:left;width:50%;}
.movie_section>a:last-child{display:block;float:right;width:50%;}
.movie_section>a:last-child>img{border:1px solid #dcdcdc;border-top:none;}
.footer_section{}
.footer_section_top{padding:2% 16% 1%;background:url(image/footer.gif) 0 0 no-repeat;-webkit-background-size: cover;
background-size: cover;}
.footer_section_top::after{content:'';clear:both;display:block;}
.fleft{float:left;width:50%;}
.fleft_nav{overflow: hidden;}
.fleft_nav>a{color:#555555;display:block;float:left;}
.fleft>h6>a{display:inline-block;margin-top:20px;}
.fleft>p{color:#8b8b8b;font-size:1.2rem;margin-top:10px;}
.fright{float:right;width:50%;text-align: right;}
.hdcgroup{position: relative;padding:6px 16px;border:1px solid #7d7d7d;
display:inline-block;}
.hdcgroup.on{border: 1px solid #d62619;background-color: #d62619;}
.hdcgroup .hdcgroup_btn{font-size:1.5rem;display:block;padding-right:16px;
background:url(image/up.png) 100% 50% no-repeat;}
.hdcgroup .hdcgroup_btn.on{background:url(image/down.png) 100% 50% no-repeat;color:#fff;}
.group_list{position: absolute;bottom:100%;right:-1px;border: 1px solid #d62619;width:960px;padding:10%;background-color: #fff;overflow: hidden;
display:none;text-align: left;}
.group_list>li{width:25%;float:left;}
.group_list>li>a{display:block;padding:10%;}
.fsns{text-align: right;margin-top:15px;}
.fsns>a{padding-left:6px;}
.copyright{background: #55585f;color: #b7b8ba;font-size:1.2rem;
text-align: center;padding:6px 0;}


